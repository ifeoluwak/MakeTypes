{
    "compilerOptions": {
        "module": "CommonJS",
        "noImplicitAny": true,
        "noUnusedLocals": false,
        "noEmitOnError": true,
        "inlineSourceMap": true,
        "strictNullChecks": true,
        "lib": [
            "es6", "es2015.collection"
        ],
        "declaration": true,
    },
    "include": [
        "*.ts",
        "generated/*.ts"
    ]
}